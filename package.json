{
  "name": "arq-node-api",
  "version": "1.0.0",
  "description": "Arquitetura NodeJS",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/notreniev/arq-node.git"
  },
  "scripts": {
    "testold": "npm run test",
    "test": "ts-node jest.startup.ts",
    "dev": "NODE_ENV=development node dist/index.js",
    "hml": "NODE_ENV=homologation nodemon dist/index.js"
  },
  "keywords": [
    "Arquitetura",
    "Node"
  ],
  "author": "Everton Canez",
  "license": "ISC",
  "jest": {
    "globals": {
      "address": "http://127.0.0.1:3100"
    },
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^12.12.14",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "mongoose": "^5.11.15",
    "pg": "^7.14.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2",
    "soap": "^0.36.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/express": "^4.17.2",
    "@types/validator": "^12.0.1"
  }
}
